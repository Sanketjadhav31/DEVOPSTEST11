pipeline {
    agent any
    tools {
       maven 'maven' 
    }
    stages { 
        //- Clone the repository using Git.  
       stage('Clone') {
            steps {
                git 'https://github.com/Sanketjadhav31/DEVOPSTEST11.git' branch: 'main'
            }
       }
       //Install dependencies using:  
       // sh 'npm install'
            stage('Dependencies') {
                steps {
                    sh 'npm install'
            }                
            }
            // Run tests with:  
            //sh 'npm test || echo "No tests defined"'
      
        stage('Test') {
            steps {
               sh 'npm test || echo "No tests defined"'
            }
        }
        stage('bild') {
            steps {
                sh 'npm run build'
            }
        }

      }
    }
